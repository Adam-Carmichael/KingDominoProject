<Window x:Class="KingDomino.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KingDomino"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="720">
    <Grid>
        <Image x:Name="image" HorizontalAlignment="Left" Height="689" VerticalAlignment="Top" Source="Resources/Misc/Box Top.png" Width="712" Stretch="Fill" />
        <Button x:Name="Start" Content="Start Game" BorderThickness="3" HorizontalAlignment="Left" Margin="275,300,0,0" VerticalAlignment="Top" Click="Start_Click" Height="60" Width="150" FontSize="20" FontFamily="Showcard Gothic"/>
        <Button x:Name="Join" Content="Join Game" BorderThickness="3" HorizontalAlignment="Left" Margin="275,400,0,0" VerticalAlignment="Top" Click="Join_Click" Height="60" Width="150" FontSize="20" FontFamily="Showcard Gothic"/>
        <Button x:Name="Quit" Content="Quit" BorderThickness="3" HorizontalAlignment="Left" Margin="275,500,0,0" VerticalAlignment="Top" Click ="Quit_Click" Height="60"  Width="150" FontSize="20" FontFamily="Showcard Gothic"/>

    </Grid>
</Window>
