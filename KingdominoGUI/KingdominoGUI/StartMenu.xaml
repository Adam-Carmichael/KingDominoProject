<Window x:Class="KingdominoGUI.StartMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KingdominoGUI"
        mc:Ignorable="d"
        Title="StartMenu" Height="720" Width="720">
    <Grid>
        <Image x:Name="BoxTop" HorizontalAlignment="Left" Height="720" VerticalAlignment="Top" Width="720" Source="..\KingdominoGUI\resources\Misc\Box Top.png"/>
        <Button x:Name="Start" BorderThickness="3" Content="Start Game" HorizontalAlignment="Left" Height="60" Margin="250,298,0,0" VerticalAlignment="Top" Width="220" FontSize="20" FontFamily="Showcard Gothic"/>
        <Button x:Name="Join" BorderThickness="3" Content="Join Game" HorizontalAlignment="Left" Height="60" Margin="250,400,0,0" VerticalAlignment="Top" Width="220" FontFamily="Showcard Gothic" FontSize="20"/>
        <Button x:Name="Quit" BorderThickness="3" Content="Quit" HorizontalAlignment="Left" Height="60" Margin="250,500,0,0" VerticalAlignment="Top" Width="220" FontFamily="Showcard Gothic" FontSize="20"/>
    </Grid>
</Window>
