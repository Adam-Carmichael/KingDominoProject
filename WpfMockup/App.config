<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <bindings>
      <netPeerTcpBinding>
        <binding name="UnsecBinding">
          <resolver mode="Pnrp" />
          <security mode="None" />
        </binding>
        <binding name="SecBinding">
          <resolver mode="Pnrp" />
          <security mode="Transport">
            <transport credentialType="Password"/>
          </security>
        </binding>
      </netPeerTcpBinding>
    </bindings>
    <client>
      <endpoint name="KDPeerEndpoint" address="net.p2p://KDPeerGames"
                binding="netPeerTcpBinding" bindingConfiguration="UnsecBinding"
                contract="PeerManager.IPeerService" kind="" endpointConfiguration="" />
    </client>
    <services>
      <service name="PeerManager.PeerService">
        <endpoint address="net.p2p://KDPeerGames" binding="netPeerTcpBinding"
          bindingConfiguration="UnsecBinding" name="KDPeerService" contract="PeerManager.IPeerService" />
      </service>
    </services>
  </system.serviceModel>
</configuration>